pipeline {
    agent any
    stages {
        stage('Git Checkout') {
            steps {
                echo 'Git Checkout'
                git branch: 'main', credentialsId: 'gitpwd', url: 'https://github.com/nareshreddy1238/Javaweb.git'
            }
        }
        stage('Validate') {
            steps {
                echo 'Validating..'
                sh 'mvn compile'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                sh 'mvn test'
            }
        }
        stage('Package') {
            steps {
                echo 'Package....'
                sh 'mvn package'
            }
        }
    }
}
